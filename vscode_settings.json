{
  "vim.mouseSelectionGoesIntoVisualMode": false,
  "vim.handleKeys": {
    "<C-s>": false,
    "<C-z>": false,
    "<C-f>": false,
    "<C-c>": false,
    "<C-v>": false,
    "<C-w>": false,
    "<C-x>": false,
    "<C-y>": false,
    "<C-a>": false,
    "<C-b>": false,
    "<backspace>": false,
  },

  // MULTI-COMMAND
  "multiCommand.commands": {
    "extension.multiCommand.ctrld": {
        "interval": 30, // Ne fonctionne pas sans cet interval
        "sequence": [
            "toggleVim",
            "editor.action.addSelectionToNextFindMatch"
        ]
    }
  },

  // VIM
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["l"],
      "after": ["j"]
    },
    {
      "before": ["k"],
      "after": ["h"]
    },
    {
      "before": ["m"],
      "after": ["l"]
    },
    {
      "before": ["o"],
      "after": ["k"]
    },
    {
      "before": ["<C-m>"],
      "after": ["w"]
    },
    {
      "before": ["<C-k>"],
      "after": ["b"]
    },
    {
      "before": [" "],
      "commands": ["toggleVim"],
      "after": ["a"]
    },
    {
      "before": ["q"],
      "commands": ["toggleVim"],
      "after": ["i"]
    },
    {
      "before": ["i"],
      "after": ["^"]
    },
    {
      "before": ["p"],
      "after": ["$"]
    },
    {
      "before": ["<C-o>"],
      "after": ["<C-u>", "z", "z"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<C-d>", "z", "z"]
    },
    {
      "before": ["O"],
      "commands": ["cursorUpSelect"]
    },
    {
      "before": ["L"],
      "commands": ["cursorDownSelect"]
    },
    {
      "before": ["r"],
      "after": [";"]
    },
    {
      "before": ["v"],
      "after": ["V"]
    },

    {
      "before": ["&"],
      "after": ["1"]
    },
    {
      "before": ["é"],
      "after": ["2"]
    },
    {
      "before": ["\""],
      "after": ["3"]
    },
    {
      "before": ["'"],
      "after": ["4"]
    },
    {
      "before": ["("],
      "after": ["5"]
    },
    {
      "before": ["-"],
      "after": ["6"]
    },
    {
      "before": ["è"],
      "after": ["7"]
    },
    {
      "before": ["_"],
      "after": ["8"]
    },
    {
      "before": ["ç"],
      "after": ["9"]
    },
    {
      "before": ["à"],
      "after": ["0"]
    }
  ],
