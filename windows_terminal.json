{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": "find",
      "keys": "ctrl+f"
    },
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+c"
    },
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+d"
    },
    {
      "command": "scrollToTop",
      "keys": "ctrl+up"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "clear\r"
      },
      "keys": "ctrl+m",
      "name": "definitive clear"
    },
    {
      "command": {
        "action": "scrollUp"
      },
      "keys": "ctrl+o"
    },
    {
      "command": "toggleFocusMode",
      "keys": "ctrl+e"
    },
    {
      "command": "scrollToBottom",
      "keys": "ctrl+down"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "clear\r\u001b[A\u001b[A\r"
      },
      "keys": "ctrl+j",
      "name": "clear then repeat last command"
    },
    {
      "command": "closePane",
      "keys": "ctrl+w"
    },
    {
      "command": "unbound",
      "keys": "ctrl+shift+end"
    },
    {
      "command": "unbound",
      "keys": "ctrl+shift+t"
    },
    {
      "command": "unbound",
      "keys": "ctrl+shift+f"
    },
    {
      "command": "unbound",
      "keys": "ctrl+shift+down"
    },
    {
      "command": "unbound",
      "keys": "ctrl+shift+up"
    },
    {
      "command": "unbound",
      "keys": "ctrl+g"
    },
    {
      "command": "unbound",
      "keys": "shift+g"
    },
    {
      "command": {
        "action": "newTab"
      },
      "keys": "ctrl+n"
    },
    {
      "command": "scrollDownPage"
    },
    {
      "command": {
        "action": "scrollDown"
      },
      "keys": "ctrl+l"
    }
  ],
  "copyFormatting": "none",
